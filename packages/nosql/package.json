{
  "name": "@ignisia/nosql",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.2.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.cjs"
    },
    "./**/*": {
      "types": "./dist/esm/**/*.d.ts",
      "import": "./dist/esm/**/*.js",
      "require": "./dist/cjs/**/*.cjs"
    },
    "./package.json": "./package.json",
    "./documents": {
      "types": "./dist/esm/documents/index.d.ts",
      "import": "./dist/esm/documents/index.js",
      "require": "./dist/cjs/documents/index.cjs"
    },
    "./documents/database": {
      "types": "./dist/esm/documents/database/index.d.ts",
      "import": "./dist/esm/documents/database/index.js",
      "require": "./dist/cjs/documents/database/index.cjs"
    },
    "./documents/document": {
      "types": "./dist/esm/documents/document/index.d.ts",
      "import": "./dist/esm/documents/document/index.js",
      "require": "./dist/cjs/documents/document/index.cjs"
    },
    "./documents/field": {
      "types": "./dist/esm/documents/field/index.d.ts",
      "import": "./dist/esm/documents/field/index.js",
      "require": "./dist/cjs/documents/field/index.cjs"
    },
    "./documents/query": {
      "types": "./dist/esm/documents/query/index.d.ts",
      "import": "./dist/esm/documents/query/index.js",
      "require": "./dist/cjs/documents/query/index.cjs"
    },
    "./key-value": {
      "types": "./dist/esm/key-value/index.d.ts",
      "import": "./dist/esm/key-value/index.js",
      "require": "./dist/cjs/key-value/index.cjs"
    }
  },
  "scripts": {
    "build": "bun run build.ts"
  },
  "devDependencies": {
    "@ignisia/utils": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist"
  ]
}