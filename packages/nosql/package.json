{
  "name": "@ignisia/nosql",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "version": "0.2.2",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    },
    "./**/*": {
      "types": "./dist/esm/**/*.d.ts",
      "import": "./dist/esm/**/*.js",
      "require": "./dist/cjs/**/*.js"
    },
    "./package.json": "./package.json",
    "./documents": {
      "types": "./dist/esm/documents/index.d.ts",
      "import": "./dist/esm/documents/index.js",
      "require": "./dist/cjs/documents/index.js"
    },
    "./documents/database": {
      "types": "./dist/esm/documents/database/index.d.ts",
      "import": "./dist/esm/documents/database/index.js",
      "require": "./dist/cjs/documents/database/index.js"
    },
    "./documents/document": {
      "types": "./dist/esm/documents/document/index.d.ts",
      "import": "./dist/esm/documents/document/index.js",
      "require": "./dist/cjs/documents/document/index.js"
    },
    "./documents/field": {
      "types": "./dist/esm/documents/field/index.d.ts",
      "import": "./dist/esm/documents/field/index.js",
      "require": "./dist/cjs/documents/field/index.js"
    },
    "./documents/query": {
      "types": "./dist/esm/documents/query/index.d.ts",
      "import": "./dist/esm/documents/query/index.js",
      "require": "./dist/cjs/documents/query/index.js"
    },
    "./key-value": {
      "types": "./dist/esm/key-value/index.d.ts",
      "import": "./dist/esm/key-value/index.js",
      "require": "./dist/cjs/key-value/index.js"
    }
  },
  "scripts": {
    "build": "bun run build.ts"
  },
  "devDependencies": {
    "@ignisia/utils": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krsbx/aio-web.git"
  },
  "bugs": {
    "url": "https://github.com/krsbx/aio-web/issues"
  },
  "keywords": [
    "bun",
    "nosql",
    "database",
    "key-value",
    "redis",
    "mongodb"
  ]
}