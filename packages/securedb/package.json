{
  "name": "@ignisia/securedb",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.1.2",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.cjs"
    },
    "./**/*": {
      "types": "./dist/esm/**/*.d.ts",
      "import": "./dist/esm/**/*.js",
      "require": "./dist/cjs/**/*.cjs"
    },
    "./package.json": "./package.json",
    "./helper": {
      "types": "./dist/esm/helper/index.d.ts",
      "import": "./dist/esm/helper/index.js",
      "require": "./dist/cjs/helper/index.cjs"
    },
    "./nosql": {
      "types": "./dist/esm/nosql/index.d.ts",
      "import": "./dist/esm/nosql/index.js",
      "require": "./dist/cjs/nosql/index.cjs"
    },
    "./sql": {
      "types": "./dist/esm/sql/index.d.ts",
      "import": "./dist/esm/sql/index.js",
      "require": "./dist/cjs/sql/index.cjs"
    }
  },
  "scripts": {
    "build": "bun run build.ts"
  },
  "dependencies": {
    "@ignisia/encryption": "workspace:*"
  },
  "devDependencies": {
    "@ignisia/utils": "workspace:*",
    "@ignisia/sql": "workspace:*",
    "@ignisia/nosql": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist"
  ]
}