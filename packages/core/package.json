{
  "name": "@ignisia/core",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.cjs"
    },
    "./**/*": {
      "types": "./dist/esm/**/*.d.ts",
      "import": "./dist/esm/**/*.js",
      "require": "./dist/cjs/**/*.cjs"
    },
    "./package.json": "./package.json",
    "./app": {
      "types": "./dist/esm/app/index.d.ts",
      "import": "./dist/esm/app/index.js",
      "require": "./dist/cjs/app/index.cjs"
    },
    "./context": {
      "types": "./dist/esm/context/index.d.ts",
      "import": "./dist/esm/context/index.js",
      "require": "./dist/cjs/context/index.cjs"
    },
    "./middlewares": {
      "types": "./dist/esm/middlewares/index.d.ts",
      "import": "./dist/esm/middlewares/index.js",
      "require": "./dist/cjs/middlewares/index.cjs"
    },
    "./middlewares/cors": {
      "types": "./dist/esm/middlewares/cors/index.d.ts",
      "import": "./dist/esm/middlewares/cors/index.js",
      "require": "./dist/cjs/middlewares/cors/index.cjs"
    },
    "./middlewares/serve-static": {
      "types": "./dist/esm/middlewares/serve-static/index.d.ts",
      "import": "./dist/esm/middlewares/serve-static/index.js",
      "require": "./dist/cjs/middlewares/serve-static/index.cjs"
    },
    "./router": {
      "types": "./dist/esm/router/index.d.ts",
      "import": "./dist/esm/router/index.js",
      "require": "./dist/cjs/router/index.cjs"
    },
    "./router/trie": {
      "types": "./dist/esm/router/trie/index.d.ts",
      "import": "./dist/esm/router/trie/index.js",
      "require": "./dist/cjs/router/trie/index.cjs"
    }
  },
  "scripts": {
    "build": "bun run build.ts"
  },
  "devDependencies": {
    "@ignisia/utils": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist"
  ]
}