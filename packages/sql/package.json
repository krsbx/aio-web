{
  "name": "@ignisia/sql",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.2.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.cjs"
    },
    "./**/*": {
      "types": "./dist/esm/**/*.d.ts",
      "import": "./dist/esm/**/*.js",
      "require": "./dist/cjs/**/*.cjs"
    },
    "./package.json": "./package.json",
    "./column": {
      "types": "./dist/esm/column/index.d.ts",
      "import": "./dist/esm/column/index.js",
      "require": "./dist/cjs/column/index.cjs"
    },
    "./database": {
      "types": "./dist/esm/database/index.d.ts",
      "import": "./dist/esm/database/index.js",
      "require": "./dist/cjs/database/index.cjs"
    },
    "./migration": {
      "types": "./dist/esm/migration/index.d.ts",
      "import": "./dist/esm/migration/index.js",
      "require": "./dist/cjs/migration/index.cjs"
    },
    "./query": {
      "types": "./dist/esm/query/index.d.ts",
      "import": "./dist/esm/query/index.js",
      "require": "./dist/cjs/query/index.cjs"
    },
    "./table": {
      "types": "./dist/esm/table/index.d.ts",
      "import": "./dist/esm/table/index.js",
      "require": "./dist/cjs/table/index.cjs"
    }
  },
  "scripts": {
    "build": "bun run build.ts"
  },
  "devDependencies": {
    "@ignisia/utils": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist"
  ]
}