{
  "name": "@ignisia/nosql",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.2.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./**/*": {
      "types": "./dist/**/*.d.ts",
      "import": "./dist/**/*.js"
    },
    "./package.json": "./package.json",
    "./documents": {
      "types": "./dist/documents/index.d.ts",
      "import": "./dist/documents/index.js"
    },
    "./documents/database": {
      "types": "./dist/documents/database/index.d.ts",
      "import": "./dist/documents/database/index.js"
    },
    "./documents/document": {
      "types": "./dist/documents/document/index.d.ts",
      "import": "./dist/documents/document/index.js"
    },
    "./documents/field": {
      "types": "./dist/documents/field/index.d.ts",
      "import": "./dist/documents/field/index.js"
    },
    "./documents/query": {
      "types": "./dist/documents/query/index.d.ts",
      "import": "./dist/documents/query/index.js"
    },
    "./key-value": {
      "types": "./dist/key-value/index.d.ts",
      "import": "./dist/key-value/index.js"
    }
  },
  "scripts": {
    "build": "bun run build.ts"
  },
  "devDependencies": {
    "@ignisia/utils": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist"
  ]
}